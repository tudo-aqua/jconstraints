(define-sort IntArray () (Array Int Int))
(declare-fun arr1 () IntArray)
(declare-fun arr2 () IntArray)
(declare-fun x1 () Int)
(assert (= x1 3))
(declare-fun y1 () Int)
(assert (= y1 2))
(assert (= (store arr1 x1 y1) arr1))
(declare-fun x2 () Int)
(assert (= x2 3))
(declare-fun y2 () Int)
(assert (= y2 2))
(assert (= (store arr2 x2 y2) arr2))
(declare-fun z1 () Int)
(assert (= z1 (select arr1 3)))
(declare-fun z2 () Int)
(assert (= z2 (select arr2 3)))
(assert (= z1 z2))
(check-sat)
(get-model)
